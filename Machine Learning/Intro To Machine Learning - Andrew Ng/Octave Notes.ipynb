    "\n",
    "Variables\n",
    "a = 3\n",
    "a = pi;\n",
    "\n",
    "disp(sprintf('2 decimals: %0.2f', a))\n",
    "disp(sprintf('10 decimals: %0.10f', a))\n",
    "\n",
